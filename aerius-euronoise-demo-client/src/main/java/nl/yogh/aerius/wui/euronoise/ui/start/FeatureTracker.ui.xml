<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:w="urn:import:nl.yogh.gwt.wui.widget"
  xmlns:p="urn:import:nl.yogh.aerius.wui.widget" xmlns:c="urn:import:nl.yogh.aerius.wui.euronoise.component" xmlns:s="urn:import:nl.yogh.aerius.wui.euronoise.ui.start">
  <ui:with field='i18n' type='nl.yogh.aerius.wui.i18n.ApplicationMessages' />
  <ui:with field='res' type='nl.yogh.aerius.wui.resources.R.ApplicationResource' />
  <ui:style type="nl.yogh.aerius.wui.euronoise.ui.start.FeatureTracker.CustomStyle">
    .icon {
    margin-bottom: 5px;
    }
    .hwn {
    border-radius: 5px;
    font-weight: bold;
    padding: 0px 5px;
    color: white;
    background-color: #aa0000;
    }
    .image {
    width: 32px;
    height: 32px;
    background: black;
    padding-bottom: 7px;
    -webkit-mask-repeat: no-repeat;
    mask-repeat: no-repeat;
    }
    .own {
    border-radius: 5px;
    font-weight: bold;
    padding: 0px 5px;
    color: white;
    background-color: #2395f7;
    white-space: nowrap;
    overflow: hidden;
    }
    .panel {
    }
    .distribution > * {
    min-width: 40px;
    line-height: 40px;
    transition: width 0.15s ease-out;
    }
  </ui:style>
  <g:FlowPanel styleName="{res.css.flex} {res.css.grow} {style.panel}">
    <g:FlowPanel ui:field="distributionPanel" styleName="{res.css.flex} {res.css.grow} {res.css.alignCenter} {style.distribution}" />
  </g:FlowPanel>
</ui:UiBinder>
 
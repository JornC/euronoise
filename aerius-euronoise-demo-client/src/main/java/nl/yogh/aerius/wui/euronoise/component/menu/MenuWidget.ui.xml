<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:w="urn:import:nl.yogh.gwt.wui.widget"
  xmlns:p="urn:import:nl.yogh.aerius.wui.widget" xmlns:c="urn:import:nl.yogh.aerius.wui.euronoise.component" xmlns:m="urn:import:nl.yogh.aerius.wui.euronoise.component.map">
  <ui:with field='i18n' type='nl.yogh.aerius.wui.i18n.ApplicationMessages' />
  <ui:with field='res' type='nl.yogh.aerius.wui.resources.R.ApplicationResource' />
  <ui:style>
    .panel {
    width: 120px;
    background-color: white;
    }
    .icon {
    width: 48px;
    }
  </ui:style>
  <g:FlowPanel styleName="{res.css.flex} {res.css.columns} {style.panel} {res.css.alignCenter} {res.css.noShrink}">
    <g:Button text="Omgevingswet" />
    <g:FocusPanel>
      <g:Image url="{res.layers.getSafeUri.asString}" styleName="{style.icon}" />
    </g:FocusPanel>
    <g:Button text="Map layers" />
    <g:Button text="Sources" />
  </g:FlowPanel>
</ui:UiBinder>
 